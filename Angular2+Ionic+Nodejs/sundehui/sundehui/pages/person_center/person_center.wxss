.form-item-bd {
    flex: 1;
    height: 60rpx;
    padding-top: 8rpx;
}
.input {
    box-sizing: border-box;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    /* background-color: #fff; */
